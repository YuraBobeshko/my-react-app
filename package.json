{
    "name": "my-react-app",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
        "preview": "vite preview",
        "format": "prettier --write \"**/*.+(js|jsx|css|md|json)\"",
        "lint:fix": "eslint --fix .",
        "prepare": "husky install",
        "predeploy": "tsc && vite build",
        "deploy": "gh-pages -d build"
    },
    "dependencies": {
        "gh-pages": "^6.1.1",
        "leaflet": "^1.9.4",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-leaflet": "^4.2.1"
    },
    "devDependencies": {
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.0",
        "@mui/icons-material": "^5.15.11",
        "@mui/material": "^5.15.11",
        "@mui/styled-engine-sc": "^6.0.0-alpha.17",
        "@types/react": "^18.2.56",
        "@types/react-dom": "^18.2.19",
        "@typescript-eslint/eslint-plugin": "^7.0.2",
        "@typescript-eslint/parser": "^7.0.2",
        "@vitejs/plugin-react": "^4.2.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "eslint-plugin-simple-import-sort": "^12.0.0",
        "eslint-plugin-unused-imports": "^3.1.0",
        "husky": "^9.0.11",
        "lint-staged": "^15.2.2",
        "prettier": "^3.2.5",
        "styled-components": "^6.1.8",
        "typescript": "^5.2.2",
        "vite": "^5.1.4"
    },
    "lint-staged": {
        "*.+(ts|tsx)": [
            "eslint --fix --max-warnings 0",
            "git add"
        ],
        "*.+(json|css|md)": [
            "prettier --write --max-warnings 0",
            "git add"
        ]
    },
    "main": "index.js",
    "repository": "https://github.com/YuraBobeshko/my-react-app.git",
    "author": "gorobech55@gmail.com",
    "license": "MIT"
}
